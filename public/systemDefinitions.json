			"NodeHeatSeekerRobotOnLine": {
				"simpleLogic": "RobotOnLine",
				"output": { "variable": "out" },
				"inputs": [],
				"logic": "return !((JSON.parse(sensorDataRef.current).rightLineSensor<0.1) && (JSON.parse(sensorDataRef.current).leftLineSensor<0.1))",
				"await": false
